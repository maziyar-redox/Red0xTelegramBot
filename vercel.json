{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "public": true,
    "builds": [
        {
            "src": "Red0xBot/__main__.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Red0xBot/__main__.py"
        }
    ],
    "redirects": [
        {
            "source": "/",
            "destination": "/status",
            "permanent": true
        }
    ]
}