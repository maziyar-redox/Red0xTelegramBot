{
  "name": "red0x-telegram-bot",
  "version": "0.0.1",
  "description": "Personal Telegram Bot",
  "main": "src/index.ts",
  "author": "MrRed0x (https://github.com/maziyar-redox)",
  "homepage": "https://github.com/maziyar-redox/Red0xTelegramBot",
  "scripts": {
    "dev": "DEBUG=bot* dotenv -- nodemon -e ts -x ts-node src/index.ts",
    "devWindows": "@powershell -Command $env:DEBUG='bot*';dotenv -- -- nodemon -e ts -x ts-node src/index.ts",
    "build": "ncc build src/index.ts -o public -m",
    "prettier": "prettier --write 'src/**/*.ts'",
    "lint": "tsc --noemit",
    "start": "DEBUG=bot* dotenv -- nodemon -e js -x node public/index.js",
    "startWindows": "@powershell -Command $env:DEBUG='bot*';dotenv -- -- nodemon -e js -x node public/index.js"
  },
  "dependencies": {
    "@vercel/node": "^5.0.2",
    "dotenv-cli": "^8.0.0",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/node": "^22.10.5",
    "@vercel/ncc": "^0.38.3",
    "debug": "^4.4.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vercel": "^39.2.5"
  }
}
